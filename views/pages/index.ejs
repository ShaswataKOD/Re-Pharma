<% layout("/layouts/boilerplate") %>
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Mental Health Guardian</title>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
    />
    <link rel="stylesheet" href="styles.css" />
    <style>
      body {     
        margin: 0;
        padding: 0;
        text-align: center;
        background-color: #f4f4f4;
        color: black;
      }
      .hero {
        color: rgb(12, 6, 6);
        padding: 90px;
      }
      .btn {
        background-color: #18716e;
        color: white;
        padding: 10px 20px;
        border: none;
        cursor: pointer;
        border-radius: 5px;
      }
      .features,
      .how-it-works,
      .services {
        padding: 20px;
        background: white;
        margin: 20px;
        border-radius: 10px;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
      }
      .feature-cards,
      .steps,
      .service-cards {
        display: flex;
        justify-content: center;
        gap: 113px;
        flex-wrap: wrap;
      }
      .card,
      .step {
        background: #fff;
        padding: 35px;
        border-radius: 10px;
        box-shadow: 10px 10px 10px rgba(0, 0, 0, 0.1);
        text-align: center;
        width: 310px;
        align-items: center;
      }
      .card img,
      .step img {
        width: 100px;
        height: 100px;
        object-fit: contain;
      }

      .services {
        text-align: center;
        justify-content: center;
      }

      .services h2 {
        font-size: 1.8em;
        margin-bottom: 20px;
      }

      .service-card {
        display: inline-block;
        background-color: #ffffff;
        border: 1px solid #e0e0e0;
        border-radius: 10px;
        padding: 20px;
        width: calc(25% - 40px);
        margin: 10px;
        text-align: center;
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
        justify-content: center;
      }

      .icon {
        font-size: 50px;
      }

      .container {
        display: flex;
        padding: 20px;
        max-width: 1500px;
        margin: auto;
        background-color: #ffffff;
        border-radius: 8px;
        box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
      }

      .image-section {
        flex: 1;
        padding-right: 20px;
      }

      .person-image {
        width: 100%;
        border-radius: 8px;
      }

      .content-section {
        flex: 2;
      }

      h1 {
        font-size: 2.5rem;
        margin-bottom: 20px;
      }

      p {
        font-size: 1rem;
        line-height: 1.6;
        margin-bottom: 20px;
      }

      .service-box {
        background-color: #e9f7f0;
        border: 1px solid #82e1c4;
        border-radius: 8px;
        padding: 15px;
        margin-bottom: 15px;
      }

      .feedback-section {
        display: flex;
        align-items: center;
        margin-top: 20px;
      }

      .feedback-icon {
        font-size: 2rem;
        margin-right: 10px;
      }

      .user-images {
        display: flex;
        gap: 5px;
      }

      .user-image {
        width: 40px;
        height: 40px;
        border-radius: 50%;
        border: 2px solid #ffffff;
      }
    </style>
  </head>
  <body>
    <div class="about">
      <header class="hero">
        <h1>Anonymous AI-Powered Mental Health Guardian</h1>
        <p>
          Your privacy is our priority. Get the support you need without
          revealing your identity.
        </p>
        <button class="btn" id="scrollButton">Get Started</button>
      </header>
      <!-- <section id="scrollTarget" style="height: 100vh;"></section> -->

      <section class="features">
        <h2>Features</h2>
        <div class="feature-cards">
          <div class="card">
            <img
              src="data:image/jpeg;base64,"
              alt="Anonymous Support"
            />
            <h3>Anonymous Support</h3>
            <p>Get help without revealing your identity.</p>
          </div>
          <div class="card">
            <img
              src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQ7y2MtWzvF0wZRcwxBNz-I9NPdJNcYW7Ewow&s"
              alt="AI-Powered Insights"
            />
            <h3>AI-Powered Insights</h3>
            <p>Receive personalized advice and insights.</p>
          </div>
          <div class="card">
            <img
              src="data:image/jpeg;base64,"
              alt="24/7 Availability"
            />
            <h3>24/7 Availability Here</h3>
            <p>Access support anytime, anywhere.</p>
          </div>
        </div>
      </section>

      <section class="how-it-works">
        <h2>How It Works</h2>
        <div class="steps">
          <div class="step">
            <img
              src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQv4hX2exSVm1jHr9trTYJbmHqDSQNicothTA&s"
              alt="Sign Up"
            />
            <h3>Step 1: Sign Up</h3>
          </div>
          <div class="step">
            <img
              src="data:image/jpeg;base64,"
              alt="Share Your Concerns"
            />
            <h3>Step 2: Share Your Concerns</h3>
          </div>
          <div class="step">
            <img
              src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQlK-v5zfRYbb55gJNluw11MFz8FSClFuCqqbf67xc7ob7qZ5OKEKyRLYoX6Kt4-OEogWA&usqp=CAU"
              alt="Receive Support"
            />
            <h3>Step 3: Receive Support</h3>
          </div>
        </div>
      </section>

      <div class="container">
        <div class="image-section">
          <img
            src="https://dranamena.com/wp-content/uploads/2023/03/MindCare-Psychological-Services-Kids.png"
            alt="Mindcare session"
            class="person-image"
          />
        </div>
        <div class="content-section">
          <h1>We care for you, we write for you</h1>
          <p>
            Re-Pharma is a growing community working towards changing the way
            individuals think & act about problems related to Mental Health.
            Managed by the current generation.
          </p>
          <div class="service-box">
            <h3>Affordable sessions</h3>
            
          </div>
          <div class="service-box">
            <h3>Simple search process</h3>
          </div>
          <div class="service-box">
            <h3>High-quality care</h3>
          </div>
        </div>
      </div>

      <section class="services">
        <h2>What types of service you want?</h2>
        <div class="service-cards">
          <div class="service-card">
            <div class="icon">&#128100;</div>
            
            
            <a style="text-decoration: none;color: black;" href="/users/login" target="_blank"><h3>Individual Counselling</h3></a>
          </div>
          <div class="service-card">
            <div class="icon">&#128106;</div>
            
            <a style="text-decoration: none;color: black;" href="/ai/chat" target="_blank"><h3>AI Counselling</h3></a>
          </div>
          <div class="service-card">
            <div class="icon">&#128101;</div>
            
            <a style="text-decoration: none;color: black;" href="/therapists/search" target="_blank"><h3>Health Diagonosis</h3></a>
          </div>
        </div>
      </section>
    </div>
  </body>
  <script>
  document.getElementById("scrollButton").addEventListener("click", function () {
    window.scrollBy({
      top: window.innerHeight / 2, // Scroll down by half the viewport height
      behavior: "smooth"
    });
  });
</script>
</html>
